<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Date_Format_Type__c</fullName>
    <description>This is used to create the correct Date format in the Start and End Date formula fields on the Line Item, depending on the Market.</description>
    <externalId>false</externalId>
    <formula>IF(
(Market__c = "CNB") || 
(Market__c = "JPT") || 
(Market__c = "KRS") || 
(Market__c = "TWI") || 
(Market__c = "IRT") || 
(Market__c = "USB") , "DateTypeUS",
"DateTypeROW")</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Date Format Type</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
