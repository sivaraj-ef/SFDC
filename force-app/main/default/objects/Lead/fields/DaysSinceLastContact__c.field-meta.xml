<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>DaysSinceLastContact__c</fullName>
    <description>Calculates number of days since last contact with customer</description>
    <externalId>false</externalId>
    <formula>if(( LastContactDateTime__c - Now() &lt; 0), NOW() - LastContactDateTime__c , 0)</formula>
    <label>Last Contact(Days)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
