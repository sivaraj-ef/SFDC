<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>HasPQorRESInPast30Days__c</fullName>
    <description>Indicates whether the customer has a PQ or RES related to this Opportunity within the last 30 days</description>
    <externalId>false</externalId>
    <formula>NOW() - LastPQDateTime__c &lt;= 30 || NOW() - LastRESDateTime__c &lt;= 30</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Has PQ or RES in past 30 days</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
