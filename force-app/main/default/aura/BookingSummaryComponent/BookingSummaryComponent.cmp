										<lightning:input label="{!$Label.c.Home_Phone}" aura:id="path_3_Fields" pattern="[0-9]{}" name="number" value="{!v.wrapperList.personAcc.PersonHomePhone}" messageWhenBadInput="Landlinee phone number is greater than 10 digits" />
