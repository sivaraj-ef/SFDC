													<!--<lightning:input aura:id="path_1_Fields" type="date" required="true" label="DOB" name="zDOB" value="{!v.zDOB}" /> -->
													<lightning:input aura:id="path_1_Fields" required="true" label="{!$Label.c.Birthdate}" type="date" name="zDOB" value="{!v.wrapperList.personAcc.PersonBirthdate}" />
													<!-- <ui:inputDate aura:id="dob" displayDatePicker="true" value="{!v.wrapperList.personAcc.PersonBirthdate}" /> -->
												</div>
												<div class="slds-form-element slds-size_1-of-3 slds-p-right_x-small" style="margin-top:0">
													<lightning:input aura:id="ageField" label="{!$Label.c.Age}" value="{!v.wrapperList.age}" disabled="true" />
												</div>
												<div class="slds-form-element slds-size_1-of-3" style="margin-top:0">
													<lightning:select name="path_1_Fields" label="{!$Label.c.Gender}" value="{!v.pickGender}" required="true" aura:id="path_1_Fields">
														<!--<option text="{!v.pickGender}" value="{!v.pickGender}"></option>-->
                                                        <option value="">Please select</option>
                                                        <aura:iteration items="{!v.GenderList}" var="gv">
															<!--<option value="{!gv.value}">{!gv.key}</option>-->
															<option text="{!gv.key}" value="{!gv.value}" selected="{!gv.Selected}"/>
														</aura:iteration>
													</lightning:select>
												</div>
											</div>
										</div>
									</fieldset>
								</div>
							</div>
							<div class="slds-path__keys slds-form slds-form_stacked  slds-p-top_none">
								<div class="slds-form slds-form_compound">
									<fieldset class="slds-form-element">
										<div class="slds-form-element__group">
											<div class="slds-form-element__row">
												<div class="slds-form-element slds-size_1-of-1">
													<lightning:textarea label="{!$Label.c.PersonMailingStreet}" name="zAddress" value="{!v.wrapperList.personAcc.PersonMailingStreet}" placeholder="Please fill the address" />
												</div>
											</div>
											<div class="slds-grid">
												<div class="slds-form-element slds-size_1-of-2 slds-p-right_x-small" style="margin-top:0">
													<lightning:input label="{!$Label.c.PersonMailingPostalCode}" aura:id="path_1_Fields" pattern="[0-9]{}" messageWhenPatternMismatch="Numbers are allowed" name="zPostalCode" value="{!v.wrapperList.personAcc.PersonMailingPostalCode}" />
												</div>
												<div class="slds-form-element slds-size_1-of-2 slds-p-right_x-small" style="margin-top:0">
													<lightning:input label="{!$Label.c.PersonMailingCity}" aura:id="path_1_Fields" pattern="^[a-zA-Z]+$" messageWhenPatternMismatch="Only alphabets are allowed" name="zCity" value="{!v.wrapperList.personAcc.PersonMailingCity}" />
												</div>
												<!-- <div class="slds-form-element slds-size_1-of-3" style="margin-top:0"> &nbsp; </div> -->
											</div>
											<div class="slds-grid slds-p-top_small">
												<div class="slds-form-element__row slds-grow">
													<div class="slds-form-element slds-size_1-of-2  slds-p-right_x-small" style="margin-top:0">
														<lightning:input label="{!$Label.c.PersonMailingState}" aura:id="path_1_Fields" name="zState" value="{!v.wrapperList.personAcc.PersonMailingState}"></lightning:input>
													</div>
													<div class="slds-form-element slds-size_1-of-2 slds-p-right_x-small slds-m-left_none" style="margin-top:0">
														<lightning:select label="{!$Label.c.PersonMailingCountry}" name="zCountryOfResidence" required="true" aura:id="path_1_Fields" value="{!v.wrapperList.personAcc.PersonMailingCountry}">
															<!--<option text="{!v.selectedCountry}"></option>-->
                                                            <option value="">Please select</option>
                                                            <aura:iteration items="{!v.MaillingCountryList}" var="itemCountry">
																<!--<option value="{!item.value}">{!item.label}</option>-->
																<option text="{!itemCountry.key}" value="{!itemCountry.value}" selected="{!itemCountry.Selected}"/>
															</aura:iteration>
														</lightning:select>
													</div>
												</div>
											</div>
										</div>
									</fieldset>
								</div>
							</div>
						</div>
						<aura:if isTrue="{! (v.wrapperList.opp.Market__c == 'RUM') || (v.wrapperList.opp.Market__c == 'KZA') || (v.wrapperList.opp.Market__c == 'CNB') || (v.wrapperList.opp.Market__c == 'CNN') || (v.wrapperList.opp.Market__c == 'TWI') || (v.wrapperList.opp.Market__c == 'JPT') || (v.wrapperList.opp.Market__c == 'KRS') || (v.wrapperList.opp.Market__c == 'THB') }">
							<div class="slds-path__coach slds-grid">
								<div class="slds-path__keys slds-size_1-of-1">
									<div class="slds-grid slds-grid_align-spread slds-path__coach-title">
										<h2>ENGLISH DETAILS</h2>
									</div>
								</div>
							</div>
							<div class="slds-path__coach slds-grid">
								<div class="slds-path__keys slds-form slds-form_stacked slds-p-top_none">
									<div class="slds-form slds-form_compound">
										<fieldset class="slds-form-element">
											<div class="slds-form-element__group">
												<div class="slds-grid">
													<div class="slds-form-element slds-size_1-of-2 slds-p-right_x-small" style="margin-top:0">
														<lightning:input label="{!$Label.c.First_Name_English}" aura:id="path_2_Fields" pattern="^[a-zA-Z]+$" name="zEn_FirstName" value="{!v.wrapperList.personAcc.FirstNameEn__c}" />
													</div>
													<div class="slds-form-element slds-size_1-of-2 slds-p-right_x-small" style="margin-top:0">
														<lightning:input label="{!$Label.c.Last_Name_English}" aura:id="path_2_Fields" pattern="^[a-zA-Z]+$" name="zEn_LastName" value="{!v.wrapperList.personAcc.LastNameEn__c}" />
													</div>
												</div>
											</div>
										</fieldset>
									</div>
									<div class="slds-grid slds-grid slds-p-top_small">
										<div class="slds-form-element slds-size_1-of-3 slds-p-right_x-small" style="margin-top:0">
											<lightning:input label="{!$Label.c.PersonOtherPostalCode}" aura:id="path_2_Fields" pattern="[0-9]{}" messageWhenPatternMismatch="Numbers are allowed" name="zEn_PostalCode" value="{!v.wrapperList.personAcc.PersonOtherPostalCode}" />
										</div>
										<div class="slds-form-element slds-size_1-of-3 slds-p-right_x-small" style="margin-top:0">
											<lightning:input label="{!$Label.c.PersonOtherCity}" aura:id="path_2_Fields" name="zEn_City" value="{!v.wrapperList.personAcc.PersonOtherCity}" />
										</div>
										<!--<div class="slds-form-element slds-size_1-of-3 slds-p-right_x-small" style="margin-top:0"><lightning:input label="{!$Label.c.Shipping_State_Province}" aura:id="path_2_Fields" name="zEn_State" value="{!v.wrapperList.personAcc.ShippingState}" /></div>-->
									</div>
								</div>
								<div class="slds-path__keys slds-form slds-form_stacked  slds-p-top_none">
									<div class="slds-form slds-form_compound">
										<fieldset class="slds-form-element">
											<div class="slds-form-element__group">
												<div class="slds-form-element__row">
													<div class="slds-form-element slds-size_1-of-1">
														<lightning:textarea label="{!$Label.c.PersonOtherStreet}" name="zEn_Address" value="{!v.wrapperList.personAcc.PersonOtherStreet}" placeholder="Enter your address" />
													</div>
												</div>
											</div>
										</fieldset>
									</div>
								</div>
							</div>
						</aura:if>
						<div class="slds-clearfix">
							<div class="slds-float_right slds-p-right_x-small slds-m-top_large">
								<ui:button aura:id="next1" buttonTitle="Click to proceed next step" class="slds-button_brand" press="{!c.moveNext}">
									<span class="slds-text-not-selected">
										<lightning:icon size="x-small" iconName="utility:chevronright" class="icon_color_white" /> Continue 																																																																																																												
									</span>
								</ui:button>
							</div>
						</div>
					</div>
					<div aria-labelledby="path-3" class="slds-path__content slds-hide" aura:id="path-content-3" role="tabpanel">
						<div class="slds-path__coach slds-grid">
							<div class="slds-path__keys slds-size_1-of-1">
								<div class="slds-grid slds-grid_align-spread slds-path__coach-title">
									<h2>Personalize your preliminary details</h2>
									<!-- <button class="slds-button slds-button_reset slds-text-body_small">Edit</button> -->
								</div>
							</div>
						</div>
						<div class="slds-path__coach slds-grid">
							<div class="slds-path__keys slds-form slds-form_stacked slds-p-top_none">
								<div class="slds-grid">
									<div class="slds-form-element slds-size_1-of-2 slds-p-right_x-small" style="margin-top:0">
										<lightning:select label="{!$Label.c.Nationality}" value="{!v.selectedNationality}" aura:id="path_3_Fields" required="true" name="zNationality">
											<!--<option text="{!v.selectedNationality}" value="{!v.selectedNationality}"></option>-->
                                            <option value="">Please select</option>
                                            <aura:iteration items="{!v.NationalityList}" var="itemNation">
												<!--<option value="{!item.value}">{!item.label}</option>-->
												<option text="{!itemNation.key}" value="{!itemNation.value}" selected="{!itemNation.Selected}"/>
											</aura:iteration>
										</lightning:select>
									</div>
									<div class="slds-form-element slds-size_1-of-2" style="margin-top:0">
										<lightning:select label="Other nationalities" value="{!v.selectedOtherNationality}" aura:id="path_3_Fields" name="zOtherNationality">
											<!--<option text="{!v.selectedOtherNationality}" value="{!v.selectedOtherNationality}"></option>-->
                                            <option value="">Please select</option>
                                            <aura:iteration items="{!v.OtherNationalityList}" var="itemOtherNation">
												<!--<option value="{!item.value}">{!item.label}</option>-->
												<option text="{!itemOtherNation.key}" value="{!itemOtherNation.value}" selected="{!itemOtherNation.Selected}"/>
											</aura:iteration>
										</lightning:select>
									</div>
								</div>
